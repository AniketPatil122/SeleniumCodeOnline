package CoreJava;

public class VariableAndDatatype {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name ="neel patil";
		int age=13;
		double weight=30.53;
		
		System.out.println(name);
		System.out.println(age);
		System.out.println(weight+ "kg");
		
	}

}
